setenv HOME "/root"
setenv TERM "linux"
setenv PATH "/bin:/sbin:/usr/bin:/usr/sbin:/usr/local/bin:/usr/local/sbin:/usr/X11/bin"
setenv LD_LIBRARY_PATH "/lib:/usr/lib:/usr/X11/lib"
setenv XLOCALEDIR "/usr/X11/share/X11/locale"
setenv LANG "C"

set prompt='[%T]%B%N%b@%M:%B%~%b>' 
set visiblebell
set history = 256

alias ls 'ls --color'
alias l 'ls'
alias ll 'ls -l'
alias act 'cat'
alias quit 'exit'
alias hist 'history'
alias h 'history'
alias grep 'grep --colour'
alias vi 'vim'